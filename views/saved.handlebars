<h1>Saved Articles</h1>

<!-- Pass in data from server.js -->
<ul>
  {{#each savedArticles}}
  	<li>
  		<p><span class="label">Title:</span> {{this.title}}</p>
      <p><span class="label">Summary:</span> {{this.body}}</p>
      <p><a href="{{this.link}}" target="_blank">Read Full Article</a></p>
  	</li>
  {{/each}}
</ul>
